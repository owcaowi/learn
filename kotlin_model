package com.example.githubsearchtool.model

import com.github.kittinunf.fuel.core.ResponseDeserializable
import com.google.gson.Gson

data class UserResponse(

        val login: String?,
        val avatar_url: String?,
        val id: Number?,
        val followers_url: String?,

        val search_tool_label: String?

){
    class Deserializer: ResponseDeserializable<Array<UserResponse>> {
        override fun deserialize(content: String): Array<UserResponse>?
                = Gson().fromJson(content, Array<UserResponse>::class.java)
    }
}
